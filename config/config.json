{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "leave-request-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/leaveRequestForm/LeaveRequestFormWebPart.js",
          "manifest": "./src/webparts/leaveRequestForm/LeaveRequestFormWebPart.manifest.json"
        }
      ]
    },
    "dashboard-user-leave-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dashboardUserLeave/DashboardUserLeaveWebPart.js",
          "manifest": "./src/webparts/dashboardUserLeave/DashboardUserLeaveWebPart.manifest.json"
        }
      ]
    },
    "dashboard-manager-requests-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dashboardManagerRequests/DashboardManagerRequestsWebPart.js",
          "manifest": "./src/webparts/dashboardManagerRequests/DashboardManagerRequestsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "LeaveRequestFormWebPartStrings": "lib/webparts/leaveRequestForm/loc/{locale}.js",
    "DashboardUserLeaveWebPartStrings": "lib/webparts/dashboardUserLeave/loc/{locale}.js",
    "DashboardManagerRequestsWebPartStrings": "lib/webparts/dashboardManagerRequests/loc/{locale}.js"
  }
}
